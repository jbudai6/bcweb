@media only screen and (min-width: 768px) {/* 768 */
    
     /* GRID */
	.medium-11 {width: 90%;}
	.medium-10 {width: 83.3333%;}
    .medium-9 {width: 75%;}
	.medium-8 {width: 66.66666%;}
	.medium-6 {width: 50%; }
    .medium-4 {width: 33.33333%;}
	.medium-3 {width: 25%;}
	.medium-2 {width: 16.6666%;}
	.medium-0 {width: 0%}
	
	.medium-2-img {flex: 16.6666%;}
	.medium-4-img{flex:33.33333%}
	.medium-6-img{flex:50%}
	.medium-8-img{flex:66.66666%}
	.medium-9-img{flex:75%; }
	.medium-10-img {flex: 83.3333%;}
	.medium-12-img{flex:100%; }
	
	.column-left-img{
	float: left;  
	padding-left: 0;
	padding-right: 0;
	margin: 0 0 0 0;
}

.column-right-img{
	float: right;  
	padding-left: 0;
	padding-right: 0;
	margin: 0 0 0 0;
}
	
	.why-invest-med {
		background-image: url('../images/Why_invest.png');
		background-repeat: no-repeat; 
		background-size: cover;
		height:100vh;
	}
	
	.why-fund-med {
		background-image: url('../images/Why_fundraise.png');
		background-repeat: no-repeat; 
		background-size: cover;
		height:100vh;
	}
	
	.header-img-med {
		background-image: url('../images/Header_image_2.png');
		background-repeat: no-repeat; 
		background-size: cover;
		height:60vh;
	}

	
	.img-resize img {
    width: 100%;
	vertical-align: bottom;
}
	
	.center-align-small {text-align: left}
	
	.column-no-left-pad {
	float: left;  
	padding-left: 0;
	padding-right: 5vw;
	margin: 0 0 0 0;
}
	
.column-no-right-pad {
	float: left;  
	padding-left: 5vw;
	padding-right: 0;
	margin: 0 0 0 0;
}
	

	.img-small-remove{
		display: flex;
	}
	
	.img-medium-remove{
		display: none;	
	}
	
	.right-align-img-small{
		float:right;
	}
	
	.img-txt-container-header {
		display: flex;
		align-items: center;
		justify-content:center;
		flex-direction: row;
		padding-left:0;
		padding-right: 0;
		height:80vh;
	}
	
	.img-social img {
		height: 20px;
		width: 20px;
		margin-left:30px;
	}
	
	.img-container-social-small{
		display: none;
	}
	
	.img-container-social-medium {
		display: flex;
		justify-content: space-between;
		align-items: center;
	}
	
	
	.img-container-logo-small {
	  display: none;
	}	
	
	.img-container-logo-medium {
		display: flex;
		justify-content: center;
		padding-top: 6vw;
		padding-bottom: 6vw;
	}	
	
	.img-txt-container {
		display: flex;
		align-items: center;
		justify-content: start;
		flex-direction: row;
	}
	
	.img-txt-container-medium {
		display: flex;
		align-items: center;
		flex-direction: row;
		padding-left:0;
		padding-right: 0;
		padding-top: 10vh;
		padding-bottom: 10vh;
		margin: 0 0 0 0;
	}
	
	.container-geo {
		display: flex;
		align-items: center;
		justify-content: left;
		flex-direction: row;
		padding-top:25px;
		padding-bottom:50px;
		padding-left:3vw;
	}


	header{
		font-size: 24px;
		font-weight: 700;
		background-image: url('../images/Header_background.png');
		background-repeat: no-repeat;
		background-attachment: fixed;  
		background-size: cover;
		color: #FFFFFF;
		height:100vh;
	}


	main h1{
		font-size: 48px;
		line-height: 86px;
		font-weight:700;
	}
	
	main h2{
		font-size: 36px;
		line-height: 64px;
		font-weight:700;
	}
	
	nav button {display: none;}
	
	nav a {
    display: block;
    padding: .8rem 0; 
    color: #fff;
    text-align: center;
    text-decoration: none;
	width:15vw;
	}
	
    nav ul {display: block;}
    nav li {width: 25%; float: left;}
	nav li.active {background-color: rgba(0,0,0,0);}


/* slider */
.slidecontainer {
  position: relative;
  width: 90%; /* Width of the outside container */
}

} /* end of media query */